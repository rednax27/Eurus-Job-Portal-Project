<% title = "Home Page" %>

<!-- Hero Section -->
<%- include('../partials/home/hero-section'); %>

<!-- FIND JOBS -->
<section class="flex flex-col flex-wrap content-center justify-center h-[90vh]">
  <div class="">
    <div class="flex flex-col justify-center">
      <h2
        class="font-[Open Sans] text-[#4F4F6F] font-bold lg:text-7xl md:text-5xl text-3xl text-center"
      >
        Find Jobs
      </h2>
      <span
        class="font-[Open Sans] text-[#7D7D7D] font-light lg:text-4xl md:text-2xl text-lg mt-3 text-center"
        >Apply privately to 34,456 jobs with one application</span
