<% title = "Browse Candidates" %> <%- include('../partials/shared/header'); %>

<section
  class="bg-gradient-to-r from-[#087FE0] from-10% via-[#0685cc] via-30% to-[#05e6ba] to-90% h-[90vh]"
>
  <div class="flex w-full h-full">
    <div
      class="w-full flex flex-col flex-wrap content-center justify-center items-center md:items-start"
    >
      <h1
        class="w-full md:w-1/2 font-montserrat text-white text-center md:text-left font-bold md:text-5xl 2xl:text-7xl xl:text-6xl lg:text-6xl"
      >
        Browse Freelancers on Eurus
      </h1>
      <span
        class="w-1/2 font-open text-white 2xl:text-xl lg:text- font-light mt-8"
      >
        Where the world meets startups, Millions of small businesses use
        Freelancers to turn their ideas into a reality</span
      >
      <div class="w-[80%]">
        <%- include('../components/atom/Find-Jobs copy'); %>
      </div>
    </div>
    <section
      class="w-full h-screen flex-col-reverse md:flex-row justify-center items-center overflow-hidden hidden md:flex"
    >
      <div
        class="w-full md:w-1/2 flex justify-center items-center transform scale-75 md:scale-125 translate-x-20 md:translate-x-40 translate-y-0 md:-translate-y-20"
      >
        <img
          class="w-80 h-100 rounded-full"
          src="assets/images/ceo.png"
          alt=""
        />

        <!-- Inner -->
        <div
          class="hw-inner flex justify-between items-center absolute border border-gray-800 rounded-full animate-spin-slow anim-8s"
        >
          <div
            class="bg-white p-1 rounded-full custom-shadow h-10 w-10 transform -translate-x-5"
          >
            <img
              class="h-9 w-9 object-center animate-rotate-img anim-8s"
              src="assets/images/icons/Engineer-professions-jobs-worker-man.svg"
              alt=""
            />
          </div>
          <div
            class="bg-white overflow-hidden rounded-full custom-shadow h-10 w-10 transform translate-x-5"
          >
            <img
              class="h-10 w-10 object-center animate-rotate-img anim-8s"
              src="assets/images/icons/Cashier-sales assistant-supermarket-woman-professions.svg"
              alt=""
            />
          </div>
        </div>

        <!-- Outer -->
        <div
          class="hw-outer flex justify-between items-center absolute border border-gray-800 rounded-full animate-spin-slow"
        >
          <div class="flex justify-between items-center h-full w-full">
            <div
